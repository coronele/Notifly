
@{
    ViewData["Title"] = "AddMember";
}
@model int

<h1>Add Member!</h1>
<br />
<br />
<form action="../Home/AddMember" method="post">
   <label class="col-3">Member Name: </label><input type="text" name="memberName" placeholder="Member's Name" /><br />
    <label class="col-3">Phone Number: </label><input type="text" name="phoneNumber" placeholder="In this format: +1##########" />
    <br />
    <br />
    <input type="hidden" name="Groups" value="@Model" />
    <input type="hidden" name="groupId" value="@Model" />
    <input class="btn btn-primary" type="submit" value="Submit!" />
</form>

